{
  "name": "excel.to.json",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "node -r tsconfig-paths/register dist/server.js",
    "dev": "ts-node-dev --inspect --transpile-only --ignore-watch node_modules -r tsconfig-paths/register src/server.ts",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "prod": "npm run build && npm run knex:prod && npm run copy && node -r tsconfig-paths/register dist/server.js",
    "build": "npx tsc",
    "copy": "cp -r src/views dist/",
    "knex:migrate": "npx knex --knexfile src/config/knexfile.ts migrate:latest",
    "knex:prod": "npx knex --knexfile dist/config/knexfile.ts migrate:latest",
    "knex:seeds": "knex --knexfile knexfile.ts seed:run"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-flash-message": "^2.1.0",
    "express-session": "^1.17.2",
    "express-session-sqlite": "^2.0.10",
    "helmet": "^4.6.0",
    "knex": "^0.95.7",
    "multer": "^1.4.2",
    "sqlite3": "^5.0.2",
    "uuid": "^8.3.2",
    "xlsx-to-json-lc": "^0.5.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/ejs": "^3.0.7",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.3.3",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.2.0",
    "prettier": "^2.3.2",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  }
}
